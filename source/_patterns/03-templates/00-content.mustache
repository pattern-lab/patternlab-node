<div class="basicLayout">
    <div class="basicLayout--inner">
        {{> organisms-header }}

        <div class="pageContent">
            {{> molecules-block-banner }}

            <div class="pageContent__blockMedia">
                {{> molecules-block-text }}
                {{> molecules-block-media(isVerticallyCentered: true, isDisplay: true ) }}
                {{> molecules-block-media(isVerticallyCentered: true, isDisplay: true, isReversed: true) }}
                {{> molecules-block-media(isVerticallyCentered: true, isDisplay: true, isCroppedMedia: true) }}
                {{> molecules-block-media(isVerticallyCentered: true, isDisplay: true, isReversed: true, isCroppedMedia: true) }}
                {{> molecules-block-info }}
            </div>
        </div>
        {{> organisms-footer }}
    </div>
</div>



