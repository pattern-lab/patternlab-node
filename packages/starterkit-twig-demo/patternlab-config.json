{
  "engines": {
    "twig": {
      "package": "@pattern-lab/engine-twig-php",
      "fileExtensions": [
        "twig"
      ],
      "namespaces": [
        {
          "id": "atoms",
          "recursive": true,
          "paths": [
            "source/_patterns/atoms"
          ]
        },
        {
          "id": "molecules",
          "recursive": true,
          "paths": [
            "source/_patterns/molecules"
          ]
        },
        {
          "id": "organisms",
          "recursive": true,
          "paths": [
            "source/_patterns/organisms"
          ]
        },
        {
          "id": "templates",
          "recursive": true,
          "paths": [
            "source/_patterns/templates"
          ]
        },
        {
          "id": "pages",
          "recursive": true,
          "paths": [
            "source/_patterns/pages"
          ]
        },
        {
          "id": "macros",
          "recursive": true,
          "paths": [
            "source/_macros"
          ]
        }
      ]
    }
  }
}
