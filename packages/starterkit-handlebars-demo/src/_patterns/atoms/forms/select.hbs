<select class="c-select {{ additionalClasses }}" id="{{ id }}" placeholder="{{ placeholder }}" {{# multiple }}multiple{{/ multiple }} {{# disabled }}disabled{{/ disabled }}>
    {{# selectOptions }}
    <option value="{{ value }}">{{ option }}</option>
    {{/ selectOptions }}

    {{# optionGroups }}
        <optgroup label="{{ optGroupLabel }}">

            {{# selectOptions }}
            <option value="{{ value }}">{{ option }}</option>
            {{/ selectOptions }}

        </optgroup>
    {{/ optionGroups }}

</select>
