<!-- View All Patterns in a Pattern Sub-Type -->
<div class="pl-c-main">
  <!-- Index of Patterns -->
  <div class="pl-c-pattern-index">
    {% for partial in partials %}
      {% if partial.patternSectionSubgroup %}
        {% include "@uikit/patternSectionSubgroup.twig" %}
      {% else %}
        {% include "@uikit/patternSection.twig" %}
      {% endif %}
    {% endfor %}
  </div><!--end pl-c-pattern-index-->
</div><!--end pl-c-main-->
